<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
>
    <title>FormAssembly Connectors</title>
    <paths>
        <output>docs/php</output>
    </paths>
    <version number="1.0.0">
        <api>
            <source dsn=".">
                <path>src</path>
            </source>
            <ignore hidden="true" symlinks="true">
                <path>**/tests</path>
                <path>**/vendor</path>
            </ignore>
        </api>
    </version>

    <templates>
        <template name="default" />
    </templates>

    <setting name="template.color" value="deep-orange"/>

</phpdocumentor>